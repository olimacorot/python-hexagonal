[
    {
        "module": "dimecom.apps.backend.src.controller.services.service_get_controller",
        "controller": "ServiceGetController",
        "route": "/services/<id>",
        "dependency": true,
        "kwargs": "finder",
        "application": {
            "module": "dimecom.src.services.application.find.service_finder",
            "class": "ServiceFinder"
        },
        "repository": {
            "module": "dimecom.src.services.infrastructure.mysql_service_repository",
            "class": "MysqlServiceRepository"
        }
    },
    {
        "module": "dimecom.apps.backend.src.controller.services.service_put_controller",
        "controller": "ServicePutController",
        "route": "/services/<id>",
        "dependency": true,
        "kwargs": "creator",
        "application": {
            "module": "dimecom.src.services.application.create.service_creator",
            "class": "ServiceCreator"
        },
        "repository": {
            "module": "dimecom.src.services.infrastructure.mysql_service_repository",
            "class": "MysqlServiceRepository"
        }
    }
]

